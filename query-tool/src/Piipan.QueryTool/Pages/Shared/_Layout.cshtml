                                        </button>
                                        <ul id="nav-user" class="usa-nav__submenu" hidden>
                                            <li class="usa-nav__submenu-item">
                                                <a href="/.auth/logout?post_logout_redirect_uri=@(Model.BaseUrl)/SignedOut">Sign out</a>
                                            </li>
                                        </ul>
                                    </li>
                                }
                            </ul>
                        </div>
