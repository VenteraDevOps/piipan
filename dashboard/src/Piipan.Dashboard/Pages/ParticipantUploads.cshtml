@page
@model Piipan.Dashboard.Pages.ParticipantUploadsModel
@using Piipan.Dashboard.Client.Models
@using Piipan.Dashboard.Extensions

@{
  ViewData["Title"] = "National Accuracy Clearinghouse Metrics Dashboard";
  ParticipantUploadModel model = new ParticipantUploadModel() 
    { 
        ParticipantUploadResults = Model.ParticipantUploadResults,
        ParticipantUploadFilter = Model.UploadRequest,
        PageParams = Model.PageParams,
        Total = Model.TotalPages,
        Statistics = Model.UploadStatistics
    };
}
<component type="typeof(Dashboard.Client.Components.ParticipantUploads.ParticipantUploads)" render-mode="WebAssembly"
param-Upload="model" />